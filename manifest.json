{
  "name": "Becky G Duolingo",
  "version": "1.0.0",
  "description": "Replace all the cartoony images on Duolingo with Becky G!",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Becky G Duolingo"
  },
  "content_scripts": [
    {
      "matches": ["*://*.duolingo.com/*"],
      "js": ["script.js"]
    }
  ]
}
